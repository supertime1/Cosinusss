    "temperature": {
        "name": "Temperature",
        "unit": "°C",
        "desc": "The temperature measured by the sensor in °C. When the sensor is properly fit to the ear, it measures the core body temperature. If the temperature is lower than the expected bodytemperature, this can be a sign that the sensor is sitting tightly inside the ear.",
        "color": "",
        "dtype": "float16",
        "dtype_time": "float32",
        "send_json": true,
        "abbreviation": "temp",
        "box_plot": true
    },
    "transfer_rate": {
        "name": "Transfer Rate",
        "unit": "%",
        "desc": "(Not used by Two and newer products) Percentage of data that was successfully transmitted from the sensor to the receiver. The higher the loss, the more data is missing.",
        "color": "",
        "dtype": "float16",
        "dtype_time": "float32",
        "send_json": false,
        "abbreviation": "tr",
        "box_plot": false
    },
    "perfusion_ir": {
        "name": "Perfusion infrared",
        "unit": "%",
        "desc": "A ppg signal's amplitude is typically atlernating around (AC) an offset (DC). The AC part of the signal is for example used for detecting the heart beat. The ratio of AC/DC of the infrared PPG signal is called Perfusion. It could be described as an indicator of how 'strong' the pulse is that we are currently measuring and therefore an indicator for the signal quality.",
        "color": "",
        "dtype": "float16",
        "dtype_time": "float32",
        "send_json": true,
        "abbreviation": "pi",
        "box_plot": true
    },
    "counter": {
        "name": "Counter",
        "unit": null,
        "desc": "(Not used by Two and newer products) A number gets assigned to each data value to make sure none of them get lost during bluetooth data transfer.",
